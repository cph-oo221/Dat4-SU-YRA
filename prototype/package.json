{
  "name": "tts-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "main.js",
  "scripts": {
    "start": "concurrently \"npm run dev\" \"electron .\"",
    "start-linux": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && electron . --enable-speech-dispatcher\"",
